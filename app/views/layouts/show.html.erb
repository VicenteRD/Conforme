<section class="page-content">
  <div class="page-content-inner">
    <!-- Risk -->
    <div class="panel">
      <div class="panel-heading">
        <%= yield :heading_main %>
      </div>

      <div class="panel-body ">

        <div class="row margin-bottom">

          <div class="col-lg-1 text-right">
            <%= yield :keys_1 %>
          </div>
          <div class="col-lg-2">
            <%= yield :values_1 %>
          </div>

          <div class="col-lg-1 text-right">
            <%= yield :keys_2 %>
          </div>
          <div class="col-lg-2">
              <%= yield :values_2 %>
          </div>

          <div class="col-lg-1 text-right">
            <%= yield :keys_3 %>
          </div>
          <div class="col-lg-2">
            <%= yield :values_3 %>
          </div>

        </div>

      </div>
    </div>

    <div class="panel">
      <div class="panel-heading">
        <%= yield :heading_extra1 %>
      </div>

      <%= yield :body_extra1 %>

    </div>

    <div class="panel">
      <div class="panel-heading">
        <%= yield :heading_extra2 %>
      </div>

      <%= yield :body_extra2 %>

    </div>

  </div>
</section>

<div id="history-modal" class="modal fade" role="dialog">
  <div class="modal-dialog">

    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Bit√°cora</h4>
      </div>

      <div class="modal-body" id="history-modal-body">
        <%= yield :history_modal_body %>
      </div>
    </div>

  </div>
</div>

<%= yield :scripts %>


<% parent_layout 'application'%>