<section class="page-content">
  <div class="page-content-inner">
    <!-- Risk -->
    <div class="panel">
      <div class="panel-heading">
        <%= yield :heading %>
      </div>

      <div class="panel-body ">

        <div class="row margin-bottom-50">

          <div class="col-lg-1 text-right">
            <%= yield :keys_1 %>
          </div>
          <div class="col-lg-2">
            <%= yield :values_1 %>
          </div>

          <div class="col-lg-1 text-right">
            <%= yield :keys_2 %>
          </div>
          <div class="col-lg-2">
              <%= yield :values_2 %>
          </div>

          <div class="col-lg-1 text-right">
            <%= yield :keys_3 %>
          </div>
          <div class="col-lg-2">
            <%= yield :values_3 %>
          </div>

        </div>

        <%= yield :body_extra %>

        <div class="row text-center">
          <%= yield :footer %>
        </div>
      </div>
    </div>
  </div>
</section>

<% parent_layout 'application'%>